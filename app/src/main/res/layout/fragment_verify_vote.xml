<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VerifyVote"
    android:id="@+id/fingerprintSuccess_fragment"
    android:background="@color/white"
    android:visibility="gone">

    <TextView
        android:id="@+id/Text_Success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:fontFamily="@font/archivo_bold"
        android:text="Verification Successful!"
        android:layout_centerInParent="true"
        android:translationY="-100px"
        android:textColor="#000033"
        android:textSize="30dp" />

    <Button
        android:id="@+id/buttonToResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/Text_Success"
        android:text="See results"  />

</RelativeLayout>